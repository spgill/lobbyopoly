{
  "workbench.colorCustomizations": {
    "activityBar.background": "#e86c6a",
    "activityBar.activeBorder": "#75ea77",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#75ea77",
    "activityBarBadge.foreground": "#15202b",
    "titleBar.activeBackground": "#e1403e",
    "titleBar.inactiveBackground": "#e1403e99",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveForeground": "#e7e7e799",
    "statusBar.background": "#e1403e",
    "statusBarItem.hoverBackground": "#e86c6a",
    "statusBar.foreground": "#e7e7e7",
    "activityBar.activeBackground": "#e86c6a",
    "sash.hoverBorder": "#e86c6a",
    "statusBarItem.remoteBackground": "#e1403e",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799"
  },

  "editor.detectIndentation": true,
  "javascript.validate.enable": false,

  "[python]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  
  "peacock.color": "#e1403e",
  "peacock.remoteColor": "#e1403e",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "eslint.validate": ["javascript"],
  "python.analysis.typeCheckingMode": "basic",
}
